#include<stdio.h>
int main()
{
  float x,y;
  printf("Enter the x cordinate: ");
  scanf("%f",&x);
  printf("Enter the y cordinate:\n ");
  scanf("%f", &y);

  if (x>0 && y>0)
    printf("This point is in First quadrant.%f %f\n", x,y);
  else if ( x<0 && y>0)
    printf("This point is in Second quadrant.%f %f\n",x,y);
   else if ( x<0 && y<0)
    printf("This point is in Third quadrant.%f %f\n", x,y);
  else 
     printf("This point is in Fourth quardant.%f %f\n",x,y);
  return 0;
}
